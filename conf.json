{
  "tags": {
		"allowUnknownTags" : true
	},
  "templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "gulp-load-runners",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
		"linenums": false
	},
  "source": {
    "include": ["lib", "lib/loaders"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
